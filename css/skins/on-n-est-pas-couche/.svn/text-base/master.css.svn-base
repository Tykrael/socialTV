#sliderMenuContainer #sliderMenu ul li.pushButton img{opacity:1.0;}
#sliderMenuContainer #sliderMenu ul li a{display:inline-block;vertical-align:middle;margin-left:5px;}
#sliderMenuContainer #sliderMenu ul li a span{display:inline-block;margin:0px;padding:12px 7px;vertical-align:middle;position:relative;}
#sliderMenuContainer #sliderMenu ul li a span.pastille{display:none;position:absolute;background-color:#3d0068;border:3px solid #AAA;z-index:75000;bottom:0px;right:0px;min-height:8px;min-width:8px;padding:3px;margin:5px 3px;
	font-size:10px;color:#FFFFFF;text-align:center;
	-webkit-box-shadow: 2px 2px 7px 0px rgba(0, 0, 0, 0.5);box-shadow: 2px 2px 7px 0px rgba(0, 0, 0, 0.5);
	-webkit-border-radius:20px;border-radius:20px;
}
#sliderMenuContainer #sliderMenu ul li img{opacity:0.6;}
#sliderMenuContainer #sliderMenu ul li.active{}
#sliderMenuContainer #sliderMenu ul li.active img{opacity:1.0;}
#sliderMenuContainer #sliderMenu ul li.active a{background:#323232; /* Old browsers */
	background:-moz-linear-gradient(top,  #323232 0%, #4c4c4c 100%); /* FF3.6+ */
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#323232), color-stop(100%,#4c4c4c)); /* Chrome,Safari4+ */
	background:-webkit-linear-gradient(top,  #323232 0%,#4c4c4c 100%); /* Chrome10+,Safari5.1+ */
	background:-o-linear-gradient(top,  #323232 0%,#4c4c4c 100%); /* Opera 11.10+ */
	background:-ms-linear-gradient(top,  #323232 0%,#4c4c4c 100%); /* IE10+ */
	background:linear-gradient(to bottom,  #323232 0%,#4c4c4c 100%); /* W3C */
	filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#323232', endColorstr='#4c4c4c',GradientType=0 ); /* IE6-9 */
}
#sliderMenuContainer #sliderMenu .motifMenu{display:inline-block;}
#sliderMenuContainer #sliderMenu ul li.pushButton .off{display:none;}
#sliderMenuContainer #sliderMenu ul li.pushButton img{height:24px;}
#sliderMenuContainer #sliderMenu ul li img{ height:20px; }
/*---------------------*\
	EDITORIAL
\*---------------------*/
#editorial{width:650px;height:100%;margin:0px 25px;background:#ffffff;position:relative;
	background:-moz-linear-gradient(top,  #ffffff 0%, #f3f3f3 100%); /* FF3.6+ */
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#f3f3f3)); /* Chrome,Safari4+ */
	background:-webkit-linear-gradient(top,  #ffffff 0%,#f3f3f3 100%); /* Chrome10+,Safari5.1+ */
	background:-o-linear-gradient(top,  #ffffff 0%,#f3f3f3 100%); /* Opera 11.10+ */
	background:-ms-linear-gradient(top,  #ffffff 0%,#f3f3f3 100%); /* IE10+ */
	background:linear-gradient(to bottom,  #ffffff 0%,#f3f3f3 100%); /* W3C */
	filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f3f3f3',GradientType=0 ); /* IE6-9 */
	background-color:#F3F3F3;
}
#editorial #infosContainer{width:100%;height:100%;position:relative;}
#editorial #infosContainer *{display:inline-block;}
#editorial #infosContainer .thumb{position:absolute;top:-30px;left:-10px;}
#editorial #infosContainer .thumb img:first-child{margin-left:10px;display:block;}
#editorial #infosContainer .content{width:310px;margin:15px 15px 5px 170px;}
#editorial #infosContainer .content .title{display:block;font-weight:bold;padding-bottom:10px;color:#3d3d3d;font-family:"heldDI";}
#editorial #infosContainer .content .description{color:#676767;height:30px;font-size:13px;overflow:hidden;}
#editorialContainer .actions{position:absolute;top:10px;right:55px;z-index:99;}
#editorialContainer .actions .fb-like{display:block;margin-top:15px;padding-left:5px;width:93px}
#editorialContainer .actions a{display:block;border:1px solid #CCC;-webkit-border-radius:5px;border-radius:5px;padding:3px 12px;margin:5px 5px 5px 4px;font-weight:normal;font-size:12px;color:#323232;
	background-image:linear-gradient(bottom, rgb(247,247,247) 1%, rgb(255,255,255) 100%);
	background-image:-o-linear-gradient(bottom, rgb(247,247,247) 1%, rgb(255,255,255) 100%);
	background-image:-moz-linear-gradient(bottom, rgb(247,247,247) 1%, rgb(255,255,255) 100%);
	background-image:-webkit-linear-gradient(bottom, rgb(247,247,247) 1%, rgb(255,255,255) 100%);
	background-image:-ms-linear-gradient(bottom, rgb(247,247,247) 1%, rgb(255,255,255) 100%);
	background-image:-webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.01, rgb(247,247,247)),
		color-stop(1, rgb(255,255,255))
	);
}
#editorialContainer .actions a{
	text-decoration: none
}
#editorialContainer .actions img{
	vertical-align: middle;
	margin: 0 10px 0 0;
}

#editorialContainer .actions span{
	vertical-align: middle;
}
#editorial #infosContainer .actions .fb-like{}
#editorial #infosContainer .actions a *{display:inline-block;vertical-align:middle;}
#editorial #infosContainer .actions a img{margin-right:10px;height:12px;}
#editorial #infosContainer .actions a span{}
#editorial #infosContainer .postCount{position:absolute;right:-4px;top:80px;text-align:right;padding:12px 10px 0px 0px;}
#editorial #infosContainer .postCount .center{height:20px;padding:7px 10px 10px 10px;background-image:url('../../res/img/post_count_bg_center_thin.png');background-repeat:repeat-x;background-position:top;}
#editorial #infosContainer .postCount .center .label{display:none;}
#editorial #infosContainer .postCount .left{float:left;}
#editorial #infosContainer .postCount .right{float:right;}
#editorial #infosContainer .postCount span{display:inline-block;color:#FFF;font-size:20px;}
#editorial #infosContainer .postCount span.number{font-weight:bold;}
#editorial .facepileContainer{display:inline-block;position:absolute;top:82px;left:170px;height:50px;/*background-color:#555;*/}
