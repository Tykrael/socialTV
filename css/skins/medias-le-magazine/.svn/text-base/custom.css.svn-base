body{color:#000;font-family:Arial;background-color:#363636;}
#mainContent{background-color:white;}

/*TOPBAR*/
.siteHeader .program-title{font-family: "heldD"; font-size: 19px; color:#fff;}
.siteHeader .logo{background:url('./img/logo.png') no-repeat; width:87px; height:17px;margin:5px;}
.colorFiller{background-color:white;}

/* WIDGET */
#socialModuleContainer{background-color:#f3f3f3;}
#widgetReseauxSociaux .toolbar span{color:#3c0069;font-size:21px;}
#widgetReseauxSociaux span.twitterName,#widgetReseauxSociaux span.twitterUserName{color:#3c0069;}
#widgetReseauxSociaux .tabs{background-color:#f3f3f3}
#widgetReseauxSociaux .widgetSharer .title{font-size:13px;color:#242424;}
