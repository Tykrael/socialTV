.footerZone .logoZone{background:#00a8ff url('./img/logo-emission1.png') no-repeat center center;}
.footerZone .infoZone{border-left: white 5px solid;background:#00a8ff url('./img/bandeau-mlm2.jpg') no-repeat center center;}

/* VIDEO SNAP */
.snapZone,.footerZone{color: #fff;}
.liveZone, .logoZone{ background-color: #00a8ff;border-right: #fff 5px solid;}
.backZone .snapArrow, .liveZone .snapArrow, .logoZone .snapArrow{background: url('img/snap.arrow.png') no-repeat center left;}
.liveZone .snapButton{background: url('img/snap.button.png') no-repeat center center;}
.liveZone .snapText{font-size: 20px; font-family: 'heldD', arial;}
.snapButton:active, .snapButton.active{background: url('img/snap.button.active.png') no-repeat center center;}
.snapButton .loader{background: url('img/snap.spinner.png') no-repeat top;}
.snapZone .infoZone{border-left: white 5px solid;background:#00a8ff url('./img/bandeau-mlm2.jpg') no-repeat center center;}
.snapZone .backZone{background:#00a8ff url('./img/logo-mlm2.png') no-repeat center center;}
.backZone .backButton{background: url('img/snap.back.png') no-repeat top left; font-size: 21px;font-family: 'heldD';}
.playerZone{background-color: #00a8ff;}
.snapVideo{background:transparent url('img/snap.background.snapVideo.png') no-repeat center center;}
.snapVideoError, .playVideoError{color: #fff;}
.shareZone{background-color: #00a8ff;}
.shareZone .shareInfo{ font-size: 16px; font-family: 'heldD';}

/* SLIDE SONDAGE */
.slide-son .item-content{background:transparent url('img/sondage.background.png') no-repeat center center;}
.slide-son .item-content .title{color:#fefefe;font-size:20px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .reponse{color:white;font-size:15px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .item-timer span{font-size:12px;font-family:'heldD',arial,helvetica,sans-serif;color:white;}
.slide-son .item-message{color:#fff;font-size:12px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .resultat{color:white;font-size:15px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .resultat .pourcentage{border:2px solid #5c5c5c;background-color:#aeaeae;font-size:16px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .resultat .pourcentage span{font-size:30px;font-family:'heldD',arial,helvetica,sans-serif;}
.slide-son .resultat .pourcentage.most{background-color:#00a8ff;}
.slide-son .resultat .pourcentage.least{background-color:#450d4a;}

/* Slide Edito */
.slide-ed .item-content .title {font-family:'heldD',arial,helvetica,sans-serif; font-size:20px; color : #fefefe;}
.slide-ed .item-content .subtitle {font-family:'heldD',arial,helvetica,sans-serif; font-size:16px; color : #fefefe;}
.slide-ed .item-content .content { font-family: Arial; font-size:15px; color : white;}
.slide-ed .copyright, .slide .publiDate {color:white; font-family:Arial; font-size:14px;}

/* Slide Social */
.slide-soc .item-text{ font-family: Arial; color : #c6c6c6;}
.slide-soc .item-text .title {font-family:'heldD',arial,helvetica,sans-serif; font-size:30px; color : white;}
.slide-soc .item-text .nickname {font-size:20px;}
.slide-soc .item-text .time {font-size:20px;}
.slide-soc .item-text .content {font-family: Arial; font-size:24px;}

/* Liens dans les slides */
#slider-main .content a,#widgetReseauxSociaux .twit a {color: #00a8ff; text-decoration: none;}