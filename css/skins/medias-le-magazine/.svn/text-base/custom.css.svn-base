/* WIDGET */
/*======================================================================================================================================================================================================*/
/*======================================================================================================================================================================================================*/
/*--------------------------------------------------------------------------------------------*\
Colors
\*--------------------------------------------------------------------------------------------*/
#widgetReseauxSociaux .white,#popinReseauxSociaux .white{color:white;}
#widgetReseauxSociaux .tor a,#popinReseauxSociaux .tor a{color:#927321;}
#widgetReseauxSociaux .tgris,#popinReseauxSociaux .tgris{color:#666;}
#twit-color,.twit-color,
#widgetReseauxSociaux a.twit-color,
#widgetReseauxSociaux a#twit-color,
#popinReseauxSociaux #popin a.twit-color,
#popinReseauxSociaux .post a,
#popinReseauxSociaux .footer a,
#widgetReseauxSociaux a.hashtagLink{color:#1d9bd5;}
#widgetReseauxSociaux .post .author .name a,#popinReseauxSociaux .post .author .name a,
#widgetReseauxSociaux .tor,#popinReseauxSociaux .tor{color: #211f22;}
#widgetReseauxSociaux .post .author .twit,#popinReseauxSociaux .post .author .twit{color: #232520;}
/*--------------------------------------------------------------------------------------------*\
TOOLBAR
\*--------------------------------------------------------------------------------------------*/

#widgetReseauxSociaux .toolbar span{font-size:15px;}
#widgetReseauxSociaux .toolbar span.dataCount{font-size:12px;}
#widgetReseauxSociaux .tabs .tab.active{color:#333;background:#f3f3f3;}
#widgetReseauxSociaux .tabs .tab a{color:#eee;font-size:16px;}
#widgetReseauxSociaux .tabs .tab.active a{color:#333;}
/*--------------------------------------------------------------------------------------------*\
WIDGET CONTAINER
\*--------------------------------------------------------------------------------------------*/
#widgetReseauxSociaux .widgetContainer{font-family:'arial',helvetica, sans-serif;}
#widgetReseauxSociaux .post .author, #popinReseauxSociaux .post .author{font-size:12px;}
#widgetReseauxSociaux .post .author .name .fbName,#popinReseauxSociaux .post .author .name .fbName,
#widgetReseauxSociaux .post .author .name a .twitterName,#popinReseauxSociaux .post .author .name a .twitterName{font-size:18px;font-family:'heldD',arial,helvetica,sans-serif;}
#widgetReseauxSociaux .post .author .name a .twitterUserName,#popinReseauxSociaux .post .author .name a .twitterUserName{font-size:12px;font-family:'heldD',arial,helvetica,sans-serif;}
#widgetReseauxSociaux .post .author .twit,#popinReseauxSociaux .post .author .twit{font-size:14px;}
#widgetReseauxSociaux .post .footer a{color:#666666;/*background:transparent url("../images/puce.png") no-repeat scroll 0 center;*/}
#widgetReseauxSociaux .post .time,#popinReseauxSociaux .post .time{color:#242424;font-size:12px;}
#widgetReseauxSociaux .widgetSharer{font-size:13px;color:#FFF; background-color:transparent;}
#widgetReseauxSociaux .widgetSharer .shareForm .textareaWrapper{background-color:white;border:2px solid #fff;}
#widgetReseauxSociaux .widgetSharer .shareForm textarea{font-size:12px;}
#widgetReseauxSociaux .widgetSharer .shareForm textarea::-webkit-input-placeholder,
#widgetReseauxSociaux .widgetSharer .shareForm textarea:-moz-placeholder,
#widgetReseauxSociaux .widgetSharer .shareForm textarea:-ms-input-placeholder{color:#979797;}
#widgetReseauxSociaux .widgetSharer .shareForm .countdown{color:black;}
#widgetReseauxSociaux .widgetSharer .shareForm .actions .actionButton{color:#323232;font-family:arial;background-color:white;font-size:20px;}
#widgetReseauxSociaux .widgetSharer .shareForm .actions .actionButton.ok{color:#FFF;background:#aeaeae;}
#popinReseauxSociaux .mask{background-color:#000;opacity:0.3;filter:alpha(opacity=50);}
#popinReseauxSociaux #popin-wrapper{background-color:#FFF;}
#popinReseauxSociaux .footer a{font-size:12px;}
#popinReseauxSociaux .post{color:#717171;font-size:13px;}
#popinReseauxSociaux .post .author .twit{color:black;font-size:14px;}
#popinReseauxSociaux .post .author .details{font-size:10px;color:#AAAAAA;}
#popinReseauxSociaux .close{font-size:15px;color:#555;}

