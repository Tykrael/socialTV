	String.prototype.parseHashtag = function() {
		return this.replace(/[#]+[A-Za-z0-9-àáâãäåçèéêëìíîïðòóôõöùúûüýÿ_]+/g, function(t) {
			var tag = t;
			return '<a class="twit-color" href="https://twitter.com/hashtag/'+tag.split('#')[1]+'" target="_blank">'+tag+'</a>';
		});
	};
